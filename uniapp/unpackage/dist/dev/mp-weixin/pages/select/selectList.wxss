.cell-container {
    width: 100%;
    background: #FFFFFF;
    height: 3.5rem;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    line-height: 3.5rem;
    border-bottom: #F1F1F1 solid .001rem;
    font-size: 26rpx
}
.cell-container text {
    margin-left: .4rem;
    margin-right: .4rem;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.cell-container image {
    width: 2.8rem;
    height: 2.8rem;
    background-color: #eeeeee;
    margin: .3rem .3rem .3rem 1rem;
}

