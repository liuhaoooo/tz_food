













































/**header */
.headerinfo {
  height: 260rpx;
  position: fixed;
  width: 100%;
  top: 0;
}
.headerinfo ._img {
  width: 100%;
  height: 100%;
}
/**map */
.map {
  position: fixed;
  top: 260rpx;
  width: 100%;
  height: 180rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  background: #c2fceb7c;
}
/**content */
.foodList-content {
  position: absolute;
  top: 500rpx;
  width: 100%;
}
.list-item {
  height: 140rpx;
  margin-bottom: 20rpx;
  margin-top: 20rpx;
  background: #dfdfdf7c;
}
.list-item ._img {
  height: 140rpx;
  width: 160rpx;
  border-radius: 10rpx;
}

