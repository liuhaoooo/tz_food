
.cell-container{
	width: 100%;
	background: #FFFFFF;
	height: 3rem;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	line-height: 3rem;
	border-bottom: #F1F1F1 solid .001rem;
}
.cell-container text{
	margin-left: 2rem;
}
.cell-container image{
	width: 2.3rem;
	height: 2.3rem;
	background-color: #eeeeee;
	margin: .3rem .3rem .3rem 1rem;
}

