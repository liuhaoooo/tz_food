
.select-foodlist {
    height: 85vh;
}
.cell-container {
    width: 100%;
    background: #FFFFFF;
    height: 3.5rem;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    line-height: 3.5rem;
    border-bottom: #F1F1F1 solid .001rem;
}
.cell-container text {
    margin-left: .4rem;
    margin-right: .4rem;
}
.cell-container image {
    width: 2.8rem;
    height: 2.8rem;
    background-color: #eeeeee;
    margin: .3rem .3rem .3rem 1rem;
}
.footer-button {
    width: 140rpx;
    height: 140rpx;
    line-height: 140rpx;
    border-radius: 50%;
    position: fixed;
    right: 30rpx;
    bottom: 2rem;
    margin: auto;
}

